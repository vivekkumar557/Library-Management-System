<% layout('layout/boilerplate2.ejs') -%>
<div class="books">
<h1>Returned Books</h1>
    <table>
        <tr>
            <th class="bookid">Return ID</th>
            <th class="bookname">Library ID</th>
            <th class="Author">Student Name</th>
            <th class="isbn">Branch</th>
            <th class="edition">Book Name</th>
            <th class="delete">Book ID</th>
            <th class="delete">Issue Date</th>
            <th class="delete">Return Date</th>
        </tr>
        <% for(Return of returns){ %>
            <tr>
                <td class="column1">
                 <%= Return.id  %> 
                </td>
                <td class="column2">
                    <%= Return.Libraryid  %>  
                </td>
                <td class="column3">
                    <%= Return.Studentname  %> 
                </td>
                <td class="column3">
                    <%= Return.Branch  %> 
                </td>
                <td class="column3">
                    <%= Return.Bookname  %> 
                </td>
                <td class="column3">
                    <%= Return.Bookid  %> 
                </td>
                <td class="column3">
                    <%= Return.Issuedate %> 
                </td>
                <td class="column3">
                    <%= Return.Returndate  %> 
                </td>
            </tr>
        <% } %>
    </table>
</div>
</body>
</html>